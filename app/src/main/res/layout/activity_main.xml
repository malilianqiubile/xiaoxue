<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.day05disitao.MainActivity">
<Button
    android:id="@+id/bt_tiaozhuan"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="跳转"
    android:layout_alignParentRight="true"/>
   <TextView
       android:id="@+id/tv_jiyun"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/bt_tiaozhuan"
       android:layout_centerHorizontal="true"
       android:layout_marginTop="120dp"
       android:text="欢迎来到积云教育"/>
<ImageView
    android:id="@+id/iv_jianbian"
    android:layout_width="200dp"
    android:layout_height="200dp"
    android:src="@mipmap/baobao"
    android:layout_centerHorizontal="true"
    android:layout_below="@+id/tv_jiyun"
    android:layout_marginTop="20dp"/>
</RelativeLayout>
